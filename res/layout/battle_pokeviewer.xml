<?xml version="1.0" encoding="utf-8"?>
<de.marcreichelt.android.RealViewSwitcher
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/battlePokeSwitcher"
  android:clickable="true"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
   <!-- Begin Page 1 -->
    <LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="#FFFFFF"
	android:id="@+id/battlelayout"
	android:clickable="true">
	
	<!-- START Player A Title -->
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_marginTop="3dp"
		android:layout_marginBottom="3dp"
		android:layout_marginLeft="4dp"
		android:layout_marginRight="4dp"
		android:layout_margin="4dp" 
		android:layout_weight="1" 
		android:layout_height="22dp">
		
		<TextView
			android:text="Timer A"
			android:layout_height="fill_parent"
			android:layout_weight="3"
			android:textColor="@color/poke_text_color"
			android:background="@drawable/battle_border"
			android:gravity="center"
			android:id="@+id/timerA" 
			android:layout_width="fill_parent"/>
		<TextView
			android:text="Name A"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:textColor="@color/poke_text_color"
			android:gravity="right|center_vertical"
			android:id="@+id/nameA"/>
		
	</LinearLayout>
	<!-- END Player A Title -->
	
	<!-- START Battle Screen -->
	<RelativeLayout
		android:text="Battle Screen"
		android:layout_width="fill_parent"
		android:textColor="@color/poke_text_color"
		android:background="@drawable/battle_border"
		android:layout_marginTop="0dp"
		android:layout_marginBottom="3dp"
		android:layout_marginLeft="4dp"
		android:layout_marginRight="4dp"
		android:id="@+id/battleScreen"  
		android:layout_height="150dp">
				<ImageView
					android:id="@+id/pokeSpriteA"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_alignParentRight="true"
					android:layout_alignParentTop="true"/>
				<RelativeLayout
					android:layout_height="38dp"
					android:layout_width="fill_parent"
					android:background="@drawable/battle_border"
					android:layout_toLeftOf="@id/pokeSpriteA">
					<com.pokebros.android.pokemononline.TextProgressBar
				android:indeterminateOnly="false"
				android:indeterminateDrawable="@android:drawable/progress_indeterminate_horizontal"
				android:layout_height="10dp" 
				android:layout_width="fill_parent"
				android:id="@+id/hpBarA"
				android:layout_alignParentTop="true"
				android:progressDrawable="@drawable/green_progress"/>
				<TextView
				android:layout_width="wrap_content"
				android:textColor="@color/poke_text_color"
				android:id="@+id/currentPokeNameA"
				android:layout_height="wrap_content"
				android:layout_alignParentLeft="true"
				android:layout_below="@id/hpBarA"/>
				<TextView
				android:layout_width="wrap_content"
				android:textColor="@color/poke_text_color"
				android:id="@+id/currentPokeLevelA"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_below="@id/hpBarA"
				android:layout_marginLeft="2dp"/>
				<ImageView
				android:layout_width="wrap_content"
				android:id="@+id/currentPokeGenderA"
				android:layout_height="wrap_content"
				android:layout_toLeftOf="@id/currentPokeLevelA"
				android:layout_below="@id/hpBarA"
				android:layout_marginLeft="2dp"
				android:layout_marginTop="3dp"/>
				<ImageView
				android:layout_width="wrap_content"
				android:id="@+id/currentPokeStatusA"
				android:layout_height="wrap_content"
				android:layout_toLeftOf="@id/currentPokeGenderA"
				android:layout_below="@id/hpBarA"
				android:layout_marginTop="3dp"/>
				</RelativeLayout>
				<ImageView
					android:id="@+id/pokeSpriteB"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_alignParentBottom="true"
					android:layout_alignParentLeft="true"/>
			<RelativeLayout
					android:layout_height="38dp"
					android:layout_width="fill_parent"
					android:background="@drawable/battle_border"
					android:layout_toRightOf="@id/pokeSpriteB"
					android:layout_alignParentBottom="true">
			<com.pokebros.android.pokemononline.TextProgressBar
				android:indeterminateOnly="false"
				android:indeterminateDrawable="@android:drawable/progress_indeterminate_horizontal"
				android:layout_height="10dp" 
				android:layout_width="fill_parent"
				android:id="@+id/hpBarB"
				android:layout_alignParentBottom="true"
				android:progressDrawable="@drawable/green_progress"/>
			<TextView
				android:textColor="@color/poke_text_color"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/currentPokeNameB"
				android:layout_alignParentLeft="true"
				android:layout_above="@id/hpBarB"/>
			<TextView
				android:textColor="@color/poke_text_color"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/currentPokeLevelB"
				android:layout_alignParentRight="true"
				android:layout_above="@id/hpBarB"
				android:layout_marginLeft="2dp"/>
			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/currentPokeGenderB"
				android:layout_toLeftOf="@id/currentPokeLevelB"
				android:layout_above="@id/hpBarB"
				android:layout_marginLeft="2dp"
				android:layout_marginBottom="3dp"/>
			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/currentPokeStatusB"
				android:layout_toLeftOf="@id/currentPokeGenderB"
				android:layout_above="@id/hpBarB"
				android:layout_marginBottom="3dp"/>
			</RelativeLayout>
					<!-- android:layout_alignBottom="@id/hpBarB"-->
	</RelativeLayout>
	<!-- END Battle Screen -->
	
	<!-- START Player B Title -->
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_marginTop="0dp"
		android:layout_marginBottom="3dp"
		android:layout_marginLeft="4dp"
		android:layout_marginRight="4dp"
		android:layout_margin="4dp" 
		android:layout_weight="1" android:layout_height="22dp">
		
		<TextView
			android:text="Name B"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:textColor="@color/poke_text_color"
			android:gravity="left|center_vertical"
			android:id="@+id/nameB"/>
		<TextView
			android:text="Timer B"
			android:layout_height="fill_parent"
			android:layout_weight="3"
			android:textColor="@color/poke_text_color"
			android:background="@drawable/battle_border"
			android:gravity="center"
			android:id="@+id/timerB" 
			android:layout_width="fill_parent"/>
	</LinearLayout>
	<!-- END Player B Title -->
	
	<!-- START Info Window -->
	<ScrollView android:layout_width="fill_parent" 
	android:id="@+id/infoScroll" 
	android:padding="5dp" 
	android:background="@drawable/battle_border" 
	android:layout_marginLeft="4dp" 
	android:layout_marginRight="4dp" 
	android:layout_marginTop="1dp" 
	android:layout_weight="40"
	android:layout_height="0dp">
		<TextView android:layout_width="fill_parent" 
		android:id="@+id/infoWindow" 
		android:textColor="@color/poke_text_color" 
		android:text="Info Box" 
		android:layout_height="wrap_content">
		</TextView>
	</ScrollView>
	<!-- END Info Window -->
	
	<!-- START Action Buttons -->
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="180dp"
		android:layout_marginTop="4dp"
		android:orientation="horizontal"
		android:id="@+id/struggleRowLayout"
		android:visibility="gone">		
		<RelativeLayout
			android:id="@+id/struggleLayout"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/curse_type_button"
			android:layout_alignParentTop="true"
			android:layout_marginTop="3dp"
			android:layout_marginBottom="3dp"
			android:layout_marginRight="3dp"
			android:layout_marginLeft="3dp"
			android:clickable="true">
			<TextView
				android:text="STRUGGLE"
				android:textSize="30sp"
				android:layout_marginTop="20dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/struggleName"
				android:textColor="@color/poke_text_color"
				android:layout_centerHorizontal="true"
				android:textStyle="bold"/>
		</RelativeLayout>
		</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="80dp"
		android:layout_marginTop="4dp"
		android:orientation="horizontal"
		android:id="@+id/topAttackRowLayout">		
		<RelativeLayout
			android:id="@+id/attack1Layout"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/battle_border_button"
			android:layout_alignParentTop = "true"
			android:layout_marginTop="3dp"
			android:layout_marginBottom="3dp"
			android:layout_marginRight="3dp"
			android:layout_marginLeft="3dp">
			<TextView
				android:text="Attack 1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack1Name"
				android:textColor="@color/poke_text_color"
				android:layout_marginTop="10dp"
				android:layout_centerHorizontal="true"
				android:textStyle="bold"/>
			<TextView
				android:text="PP"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack1PP"
				android:textColor="@color/pp_text_color"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"/>
		</RelativeLayout>
		<RelativeLayout
			android:id="@+id/attack2Layout"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/battle_border_button"
			android:layout_marginTop="3dp"
			android:layout_marginRight="3dp"
			android:layout_marginLeft="3dp"
			android:layout_marginBottom="3dp">
			<TextView
				android:text="Attack 2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack2Name"
				android:layout_marginTop="10dp"
				android:textColor="@color/poke_text_color"
				android:layout_centerHorizontal="true"
				android:textStyle="bold"/>
			<TextView
				android:text="PP"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack2PP"
				android:textColor="@color/pp_text_color"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"/>
		</RelativeLayout>
	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="70dp"
		android:layout_marginTop="4dp"
		android:orientation="horizontal"
		android:id="@+id/bottomAttackRowLayout"
		android:layout_weight="1">
		
		<RelativeLayout
			android:layout_below="@id/attack1Layout"
			android:id="@+id/attack3Layout"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/battle_border_button"
			android:layout_marginBottom="3dp"
			android:layout_marginRight="3dp"
			android:layout_marginLeft="3dp">
			<TextView
				android:text="Attack 3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack3Name"
				android:textColor="@color/poke_text_color"
				android:layout_marginTop="10dp"
				android:layout_centerHorizontal="true"
				android:textStyle="bold"/>
			<TextView
				android:text="PP"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/pp_text_color"
				android:id="@+id/attack3PP"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"/>
		</RelativeLayout>
		<RelativeLayout
			android:layout_below="@id/attack2Layout"
			android:layout_toRightOf="@id/attack3Layout"
			android:id="@+id/attack4Layout"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/battle_border_button"
			android:layout_marginBottom="3dp"
			android:layout_marginRight="3dp"
			android:layout_marginLeft="3dp">
			<TextView
				android:text="Attack 4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack4Name"
				android:layout_marginTop="10dp"
				android:textColor="@color/poke_text_color"
				android:layout_centerHorizontal="true"
				android:textStyle="bold"/>
			<TextView
				android:text="PP"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/attack4PP"
				android:textColor="@color/pp_text_color"
				android:layout_alignParentBottom="true"
				android:layout_alignParentRight="true"/>
		</RelativeLayout>
	</LinearLayout>
</LinearLayout>

	<!-- END Action Buttons -->
<!-- END Page 1 -->
	
<!-- Begin Page 2 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent" android:background="#FFFFFF"
	android:layout_height="fill_parent">
	<RelativeLayout android:background="@drawable/battle_border_button"
		android:layout_height="wrap_content" android:layout_weight="16"
		android:layout_width="fill_parent" android:id="@+id/pokeViewLayout1">
		<ImageView android:src="@drawable/icon"
			android:layout_height="wrap_content" 
			android:layout_width="wrap_content" 
			android:id="@+id/pokeViewIcon1" 
			android:background="@drawable/battle_border_transparent" 
			android:paddingLeft="5dp" 
			android:paddingRight="5dp" 
			android:layout_marginRight="5dp"
			android:layout_alignParentLeft="true"
			android:layout_centerVertical="true"/>
			<TextView 
				android:layout_height="wrap_content" 
				android:text="pokename1"
				android:id="@+id/pokename1" 
				android:textColor="@color/poke_text_color" 
				android:textSize="14sp"
				android:layout_width="wrap_content" 
				android:textStyle="bold"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@id/pokeViewIcon1"/>
			<TextView 
				android:layout_height="wrap_content" 
				android:text="hp1" 
				android:textSize="14sp"
				android:id="@+id/hp1" 
				android:textColor="@color/poke_text_color"
				android:layout_width="wrap_content"
				android:layout_below="@id/pokename1"
				android:layout_toRightOf="@id/pokeViewIcon1"/>
			<TextView 
				android:layout_height="wrap_content" 
				android:text="--"
				android:id="@+id/item1"
				android:textColor="@color/poke_text_color"
				android:textSize="10sp"
				android:layout_width="wrap_content"
				android:layout_below="@id/hp1"
				android:layout_toRightOf="@id/pokeViewIcon1"/>
			<TextView 
				android:layout_height="wrap_content" 
				android:text="ability1"
				android:id="@+id/ability1" 
				android:textColor="@color/poke_text_color" 
				android:textSize="10sp"
				android:layout_width="wrap_content"
				android:layout_below="@id/item1"
				android:layout_toRightOf="@id/pokeViewIcon1"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_alignParentRight="true" 
				android:layout_marginRight="10dp" 
				android:layout_width="75dp"
				android:layout_alignTop="@id/hp1" 
				android:gravity="center"
				android:textSize="10sp"
				android:id="@+id/p1_attack2" 
				android:background="@drawable/battle_border"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_alignParentRight="true" 
				android:layout_marginRight="10dp" 
				android:layout_below="@+id/p1_attack1" 
				android:layout_marginTop="5dp" 
				android:id="@+id/p1_attack4" 
				android:textSize="10sp"
				android:layout_width="75dp"
				android:gravity="center"
				android:background="@drawable/battle_border"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_toLeftOf="@+id/p1_attack2" 
				android:layout_marginRight="10dp" 
				android:layout_alignTop="@id/p1_attack2" 
				android:id="@+id/p1_attack1" 
				android:textSize="10sp"
				android:layout_width="75dp"
				android:gravity="center"
				android:background="@drawable/battle_border"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_below="@+id/p1_attack1" 
				android:layout_toLeftOf="@+id/p1_attack4" 
				android:layout_alignTop="@id/p1_attack4" 
				android:layout_marginRight="10dp" 
				android:id="@+id/p1_attack3" 
				android:textSize="10sp"
				android:layout_width="75dp"
				android:gravity="center"
				android:background="@drawable/battle_border"/>
	</RelativeLayout>
	<RelativeLayout android:background="@drawable/battle_border_button"
		android:layout_height="wrap_content" android:layout_weight="16"
		android:layout_width="fill_parent" android:id="@+id/pokeViewLayout2">
		<ImageView android:src="@drawable/icon"
			android:layout_height="wrap_content" 
			android:layout_centerVertical="true"
			android:layout_width="wrap_content" 
			android:id="@+id/pokeViewIcon2" 
			android:background="@drawable/battle_border_transparent" 
			android:paddingLeft="5dp" 
			android:paddingRight="5dp" 
			android:layout_marginRight="5dp"
			android:layout_alignParentLeft="true"/>
			<TextView 
				android:layout_height="wrap_content" 
				android:text="pokename1"
				android:id="@+id/pokename2" 
				android:textColor="@color/poke_text_color" 
				android:textSize="14sp"
				android:layout_width="wrap_content" 
				android:textStyle="bold"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@id/pokeViewIcon2"/>
			<TextView 
				android:layout_toRightOf="@id/pokeViewIcon2"
				android:layout_height="wrap_content" 
				android:text="hp1" 
				android:textSize="14sp"
				android:id="@+id/hp2" 
				android:textColor="@color/poke_text_color"
				android:layout_width="wrap_content"
				android:layout_below="@id/pokename2"/>
			<TextView 
				android:layout_toRightOf="@id/pokeViewIcon2"
				android:layout_height="wrap_content" 
				android:text="--"
				android:id="@+id/item2"
				android:textColor="@color/poke_text_color"
				android:textSize="10sp"
				android:layout_width="wrap_content"
				android:layout_below="@id/hp2"/>
			<TextView 
				android:layout_toRightOf="@id/pokeViewIcon2"
				android:layout_height="wrap_content" 
				android:text="ability1"
				android:id="@+id/ability2" 
				android:textColor="@color/poke_text_color" 
				android:textSize="10sp"
				android:layout_width="wrap_content"
				android:layout_below="@id/item2"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_width="75dp" 
				android:layout_alignParentRight="true" 
				android:layout_marginRight="10dp" 
				android:layout_marginTop="5dp" 
				android:layout_alignTop="@id/hp2" 
				android:id="@+id/p2_attack2" 
				android:gravity="center"
				android:textSize="10sp"
				android:background="@drawable/battle_border"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_width="75dp" 
				android:layout_alignParentRight="true" 
				android:layout_marginRight="10dp" 
				android:layout_below="@+id/p2_attack1" 
				android:gravity="center"
				android:layout_marginTop="5dp"
				android:id="@+id/p2_attack4" 
				android:textSize="10sp"
				android:background="@drawable/battle_border"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_width="75dp" 
				android:layout_toLeftOf="@+id/p2_attack2" 
				android:gravity="center"
				android:layout_marginRight="10dp" 
				android:layout_alignTop="@id/p2_attack2" 
				android:id="@+id/p2_attack1" 
				android:textSize="10sp"
				android:background="@drawable/battle_border"/>
			<TextView 
				android:text="TextView" 
				android:textColor="@color/poke_text_color" 
				android:layout_height="wrap_content" 
				android:layout_width="75dp" 
				android:layout_below="@+id/p2_attack1" 
				android:gravity="center"
				android:layout_toLeftOf="@+id/p2_attack4" 
				android:layout_alignTop="@id/p2_attack4" 
				android:layout_marginRight="10dp" 
				android:textSize="10sp"
				android:id="@+id/p2_attack3" 
				android:background="@drawable/battle_border"/>
	</RelativeLayout>
	<RelativeLayout android:background="@drawable/battle_border_button"
		android:layout_height="wrap_content" android:layout_weight="16"
		android:layout_width="fill_parent" android:id="@+id/pokeViewLayout3">
		<ImageView android:src="@drawable/icon"
			android:layout_centerVertical="true"
			android:layout_height="wrap_content" 
			android:layout_width="wrap_content" 
			android:id="@+id/pokeViewIcon3" 
			android:background="@drawable/battle_border_transparent" 
			android:paddingLeft="5dp" 
			android:paddingRight="5dp" 
			android:layout_marginRight="5dp"
			android:layout_alignParentLeft="true"/>
			<TextView 
				android:layout_height="wrap_content" 
				android:text="pokename1"
				android:id="@+id/pokename3" 
				android:textColor="@color/poke_text_color" 
				android:textSize="14sp"
				android:layout_width="wrap_content" 
				android:textStyle="bold"
				android:layout_alignParentTop="true"
				android:layout_toRightOf="@id/pokeViewIcon3"/>
			<TextView 
				android:layout_toRightOf="@id/pokeViewIcon3"
				android:layout_height="wrap_content" 
				android:text="hp1" 
				android:textSize="14sp"
				android:id="@+id/hp3" 
